# Copy to .env and fill with your actual project configuration
# Get Firebase keys from Firebase Console > Project Settings > General > Your apps
# Get Gemini key from Google AI Studio
# Get Stripe keys from Stripe Dashboard

# Base Configuration
VITE_USE_BASE64_UPLOADS=false
# VITE_PUBLIC_BASE_URL=https://yourdomain.com (uncomment for production)

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_ENABLE_GPT5_PREVIEW=true

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
VITE_STRIPE_PROF_MONTHLY_LINK=https://buy.stripe.com/your_prof_link
VITE_STRIPE_ADV_MONTHLY_LINK=https://buy.stripe.com/your_adv_link
